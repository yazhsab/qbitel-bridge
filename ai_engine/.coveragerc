[run]
source = ai_engine
omit =
    */tests/*
    */alembic/*
    */benchmarks/*
    */__pycache__/*
    */vendor/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.
    if TYPE_CHECKING:
    @abstract
fail_under = 60
show_missing = true
precision = 2

[html]
directory = htmlcov
