# QBITEL Admin Console Environment Configuration

# API Configuration
VITE_API_BASE_URL=https://api.qbitel.local
VITE_API_VERSION=v1

# OIDC Authentication Configuration
VITE_OIDC_AUTHORITY=https://auth.qbitel.local
VITE_OIDC_CLIENT_ID=qbitel-console
VITE_OIDC_REDIRECT_URI=https://console.qbitel.local/auth/callback
VITE_OIDC_POST_LOGOUT_REDIRECT_URI=https://console.qbitel.local/
VITE_OIDC_SCOPE=openid profile email roles permissions organization

# Security Configuration
VITE_ENABLE_MFA=true
VITE_SESSION_TIMEOUT=28800000
VITE_AUDIT_ENABLED=true

# Feature Flags
VITE_ENABLE_DEVICE_MANAGEMENT=true
VITE_ENABLE_POLICY_MANAGEMENT=true
VITE_ENABLE_COMPLIANCE_REPORTING=true
VITE_ENABLE_SECURITY_MONITORING=true
VITE_ENABLE_SYSTEM_SETTINGS=true

# Development Configuration
VITE_DEV_MODE=false
VITE_DEBUG_LOGGING=false
VITE_MOCK_API=false

# Branding Configuration
VITE_APP_TITLE=QBITEL Admin Console
VITE_APP_DESCRIPTION=Quantum-Safe Load Balancer Management Interface
VITE_COMPANY_NAME=QBITEL Team
VITE_SUPPORT_EMAIL=support@qbitel.local
VITE_SUPPORT_URL=https://docs.qbitel.local

# Performance Configuration
VITE_API_TIMEOUT=30000
VITE_RETRY_ATTEMPTS=3
VITE_RETRY_DELAY=1000
VITE_CACHE_TTL=300000

# Monitoring Configuration
VITE_ENABLE_ANALYTICS=true
VITE_ANALYTICS_ENDPOINT=https://analytics.qbitel.local
VITE_ERROR_REPORTING_ENDPOINT=https://errors.qbitel.local