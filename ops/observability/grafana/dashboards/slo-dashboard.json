{
  "dashboard": {
    "title": "CRONOS AI - SLO Dashboard",
    "tags": ["cronos-ai", "slo", "sli", "production"],
    "timezone": "UTC",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "30s",
    "time": {
      "from": "now-24h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "Service Level Objectives Overview",
        "type": "stat",
        "gridPos": {"h": 4, "w": 24, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "avg(cronos_ai_slo_compliance_ratio)",
            "legendFormat": "Overall SLO Compliance"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "value",
          "orientation": "horizontal",
          "textMode": "value_and_name"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 0.95, "color": "yellow"},
                {"value": 0.99, "color": "green"}
              ]
            }
          }
        }
      },
      {
        "id": 2,
        "title": "Availability SLO (Target: 99.9%)",
        "type": "gauge",
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 4},
        "targets": [
          {
            "expr": "sum(rate(cronos_ai_http_requests_total{status=~\"2..\"}[5m])) / sum(rate(cronos_ai_http_requests_total[5m]))",
            "legendFormat": "Availability"
          }
        ],
        "options": {
          "showThresholdLabels": true,
          "showThresholdMarkers": true
        },
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "min": 0.95,
            "max": 1,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0.95, "color": "red"},
                {"value": 0.99, "color": "yellow"},
                {"value": 0.999, "color": "green"}
              ]
            }
          }
        }
      },
      {
        "id": 3,
        "title": "Latency SLO (Target: p95 < 500ms)",
        "type": "gauge",
        "gridPos": {"h": 8, "w": 6, "x": 6, "y": 4},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(cronos_ai_http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "P95 Latency"
          }
        ],
        "options": {
          "showThresholdLabels": true,
          "showThresholdMarkers": true
        },
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "min": 0,
            "max": 1,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 0.5, "color": "yellow"},
                {"value": 0.8, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 4,
        "title": "Error Rate SLO (Target: < 1%)",
        "type": "gauge",
        "gridPos": {"h": 8, "w": 6, "x": 12, "y": 4},
        "targets": [
          {
            "expr": "sum(rate(cronos_ai_http_requests_total{status=~\"5..\"}[5m])) / sum(rate(cronos_ai_http_requests_total[5m]))",
            "legendFormat": "Error Rate"
          }
        ],
        "options": {
          "showThresholdLabels": true,
          "showThresholdMarkers": true
        },
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "min": 0,
            "max": 0.05,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 0.01, "color": "yellow"},
                {"value": 0.03, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 5,
        "title": "Throughput SLO (Target: > 1000 req/s)",
        "type": "gauge",
        "gridPos": {"h": 8, "w": 6, "x": 18, "y": 4},
        "targets": [
          {
            "expr": "sum(rate(cronos_ai_http_requests_total[5m]))",
            "legendFormat": "Requests/sec"
          }
        ],
        "options": {
          "showThresholdLabels": true,
          "showThresholdMarkers": true
        },
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "min": 0,
            "max": 2000,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 500, "color": "yellow"},
                {"value": 1000, "color": "green"}
              ]
            }
          }
        }
      },
      {
        "id": 6,
        "title": "Error Budget Remaining (30 days)",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 12},
        "targets": [
          {
            "expr": "1 - (sum(rate(cronos_ai_http_requests_total{status=~\"5..\"}[30d])) / sum(rate(cronos_ai_http_requests_total[30d])))",
            "legendFormat": "Error Budget"
          },
          {
            "expr": "0.999",
            "legendFormat": "SLO Target (99.9%)"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "fillOpacity": 10
            }
          }
        }
      },
      {
        "id": 7,
        "title": "SLO Burn Rate",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 12},
        "targets": [
          {
            "expr": "(sum(rate(cronos_ai_http_requests_total{status=~\"5..\"}[1h])) / sum(rate(cronos_ai_http_requests_total[1h]))) / (1 - 0.999)",
            "legendFormat": "1h Burn Rate"
          },
          {
            "expr": "(sum(rate(cronos_ai_http_requests_total{status=~\"5..\"}[6h])) / sum(rate(cronos_ai_http_requests_total[6h]))) / (1 - 0.999)",
            "legendFormat": "6h Burn Rate"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 2, "color": "yellow"},
                {"value": 10, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 8,
        "title": "Request Success Rate by Endpoint",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 20},
        "targets": [
          {
            "expr": "sum by (endpoint) (rate(cronos_ai_http_requests_total{status=~\"2..\"}[5m])) / sum by (endpoint) (rate(cronos_ai_http_requests_total[5m]))",
            "legendFormat": "{{endpoint}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "min": 0.95,
            "max": 1,
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "fillOpacity": 10
            }
          }
        }
      },
      {
        "id": 9,
        "title": "Latency Distribution (P50, P95, P99)",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 20},
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(cronos_ai_http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "P50"
          },
          {
            "expr": "histogram_quantile(0.95, rate(cronos_ai_http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "P95"
          },
          {
            "expr": "histogram_quantile(0.99, rate(cronos_ai_http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "P99"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth"
            }
          }
        }
      },
      {
        "id": 10,
        "title": "SLO Compliance by Service",
        "type": "table",
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 28},
        "targets": [
          {
            "expr": "avg by (service) (cronos_ai_slo_compliance_ratio)",
            "format": "table",
            "instant": true
          }
        ],
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {"Time": true},
              "indexByName": {},
              "renameByName": {
                "service": "Service",
                "Value": "SLO Compliance"
              }
            }
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "custom": {
              "displayMode": "color-background"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 0.95, "color": "yellow"},
                {"value": 0.99, "color": "green"}
              ]
            }
          }
        }
      },
      {
        "id": 11,
        "title": "Apdex Score (Application Performance Index)",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 36},
        "targets": [
          {
            "expr": "(sum(rate(cronos_ai_http_request_duration_seconds_bucket{le=\"0.5\"}[5m])) + sum(rate(cronos_ai_http_request_duration_seconds_bucket{le=\"2\"}[5m])) / 2) / sum(rate(cronos_ai_http_request_duration_seconds_count[5m]))",
            "legendFormat": "Apdex"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "value"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "decimals": 3,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 0.7, "color": "yellow"},
                {"value": 0.85, "color": "green"}
              ]
            }
          }
        }
      },
      {
        "id": 12,
        "title": "Mean Time To Recovery (MTTR)",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 6, "y": 36},
        "targets": [
          {
            "expr": "avg(cronos_ai_incident_mttr_seconds)",
            "legendFormat": "MTTR"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "value"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 300, "color": "yellow"},
                {"value": 900, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 13,
        "title": "Mean Time Between Failures (MTBF)",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 36},
        "targets": [
          {
            "expr": "avg(cronos_ai_incident_mtbf_seconds)",
            "legendFormat": "MTBF"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "value"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 86400, "color": "yellow"},
                {"value": 604800, "color": "green"}
              ]
            }
          }
        }
      },
      {
        "id": 14,
        "title": "Change Failure Rate",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 18, "y": 36},
        "targets": [
          {
            "expr": "sum(cronos_ai_deployment_failures_total) / sum(cronos_ai_deployments_total)",
            "legendFormat": "Failure Rate"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "value"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 0.05, "color": "yellow"},
                {"value": 0.15, "color": "red"}
              ]
            }
          }
        }
      }
    ],
    "annotations": {
      "list": [
        {
          "name": "Deployments",
          "datasource": "Prometheus",
          "enable": true,
          "expr": "changes(cronos_ai_deployment_info[5m]) > 0",
          "iconColor": "blue",
          "tagKeys": "version"
        },
        {
          "name": "Incidents",
          "datasource": "Prometheus",
          "enable": true,
          "expr": "cronos_ai_incident_active == 1",
          "iconColor": "red",
          "tagKeys": "severity"
        }
      ]
    },
    "templating": {
      "list": [
        {
          "name": "environment",
          "type": "query",
          "datasource": "Prometheus",
          "query": "label_values(cronos_ai_http_requests_total, environment)",
          "multi": false,
          "includeAll": false
        },
        {
          "name": "service",
          "type": "query",
          "datasource": "Prometheus",
          "query": "label_values(cronos_ai_http_requests_total, service)",
          "multi": true,
          "includeAll": true
        }
      ]
    }
  }
}