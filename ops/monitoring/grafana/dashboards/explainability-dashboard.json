{
  "dashboard": {
    "title": "QBITEL - Explainability & Model Monitoring",
    "tags": ["qbitel", "explainability", "ml-monitoring"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "title": "Explanation Generation Rate",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "rate(qbitel_explanations_generated_total{status=\"success\"}[5m])",
            "legendFormat": "{{model_name}} - {{explanation_method}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"label": "Explanations/sec", "format": "short"},
          {"show": false}
        ]
      },
      {
        "id": 2,
        "title": "Explanation Success Rate",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "sum(rate(qbitel_explanations_generated_total{status=\"success\"}[5m])) / sum(rate(qbitel_explanations_generated_total[5m]))",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 0.95, "color": "yellow"},
                {"value": 0.99, "color": "green"}
              ]
            },
            "unit": "percentunit"
          }
        }
      },
      {
        "id": 3,
        "title": "Explanation Generation Latency (p95)",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(qbitel_explanation_generation_seconds_bucket[5m]))",
            "legendFormat": "{{model_name}} - p95",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.50, rate(qbitel_explanation_generation_seconds_bucket[5m]))",
            "legendFormat": "{{model_name}} - p50",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"label": "Latency (seconds)", "format": "s"},
          {"show": false}
        ],
        "alert": {
          "conditions": [
            {
              "evaluator": {"params": [0.05], "type": "gt"},
              "query": {"params": ["A", "5m", "now"]},
              "type": "query"
            }
          ],
          "name": "Explanation latency too high"
        }
      },
      {
        "id": 4,
        "title": "Model Drift Score",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
        "targets": [
          {
            "expr": "qbitel_model_drift_score",
            "legendFormat": "{{model_name}} v{{model_version}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"label": "Drift Score", "format": "short", "max": 1, "min": 0},
          {"show": false}
        ],
        "thresholds": [
          {"value": 0.05, "colorMode": "critical", "fill": true, "line": true, "op": "gt"}
        ]
      },
      {
        "id": 5,
        "title": "Model Accuracy",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 16},
        "targets": [
          {
            "expr": "qbitel_model_accuracy",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 0.90, "color": "yellow"},
                {"value": 0.95, "color": "green"}
              ]
            },
            "unit": "percentunit",
            "decimals": 2
          }
        }
      },
      {
        "id": 6,
        "title": "Average Model Confidence",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 6, "y": 16},
        "targets": [
          {
            "expr": "qbitel_model_confidence_average",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 0.70, "color": "yellow"},
                {"value": 0.85, "color": "green"}
              ]
            },
            "unit": "percentunit",
            "decimals": 2
          }
        }
      },
      {
        "id": 7,
        "title": "Drift Alerts (24h)",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 16},
        "targets": [
          {
            "expr": "increase(qbitel_drift_alerts_triggered_total[24h])",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 1, "color": "yellow"},
                {"value": 5, "color": "red"}
              ]
            },
            "unit": "short"
          }
        }
      },
      {
        "id": 8,
        "title": "Audit Trail Writes/sec",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 20},
        "targets": [
          {
            "expr": "rate(qbitel_audit_trail_writes_total[5m])",
            "legendFormat": "{{model_name}} - {{compliance_framework}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"label": "Writes/sec", "format": "short"},
          {"show": false}
        ]
      },
      {
        "id": 9,
        "title": "Audit Trail Write Latency",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 20},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(qbitel_audit_trail_write_seconds_bucket[5m]))",
            "legendFormat": "p95",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.50, rate(qbitel_audit_trail_write_seconds_bucket[5m]))",
            "legendFormat": "p50",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"label": "Latency (seconds)", "format": "s"},
          {"show": false}
        ]
      },
      {
        "id": 10,
        "title": "Human Review Rate",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 28},
        "targets": [
          {
            "expr": "rate(qbitel_human_reviews_total[1h])",
            "legendFormat": "{{model_name}} - {{review_outcome}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"label": "Reviews/hour", "format": "short"},
          {"show": false}
        ]
      },
      {
        "id": 11,
        "title": "Human Override Rate",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 28},
        "targets": [
          {
            "expr": "qbitel_human_override_rate",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 0.05, "color": "yellow"},
                {"value": 0.10, "color": "red"}
              ]
            },
            "unit": "percentunit",
            "decimals": 2
          }
        }
      },
      {
        "id": 12,
        "title": "Explanation Cache Performance",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 36},
        "targets": [
          {
            "expr": "rate(qbitel_explanation_cache_hits_total[5m])",
            "legendFormat": "Cache Hits - {{model_name}}",
            "refId": "A"
          },
          {
            "expr": "rate(qbitel_explanation_cache_misses_total[5m])",
            "legendFormat": "Cache Misses - {{model_name}}",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"label": "Requests/sec", "format": "short"},
          {"show": false}
        ]
      },
      {
        "id": 13,
        "title": "Cache Hit Rate",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 36},
        "targets": [
          {
            "expr": "rate(qbitel_explanation_cache_hits_total[10m]) / (rate(qbitel_explanation_cache_hits_total[10m]) + rate(qbitel_explanation_cache_misses_total[10m]))",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 0.50, "color": "yellow"},
                {"value": 0.80, "color": "green"}
              ]
            },
            "unit": "percentunit",
            "decimals": 2
          }
        }
      }
    ],
    "templating": {
      "list": [
        {
          "name": "model_name",
          "type": "query",
          "query": "label_values(qbitel_model_drift_score, model_name)",
          "refresh": 1,
          "multi": true,
          "includeAll": true
        }
      ]
    },
    "time": {
      "from": "now-6h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["10s", "30s", "1m", "5m", "15m", "30m", "1h"]
    },
    "annotations": {
      "list": [
        {
          "name": "Drift Alerts",
          "datasource": "Prometheus",
          "expr": "ALERTS{alertname=\"ModelDriftDetected\"}",
          "tagKeys": "model_name,severity",
          "titleFormat": "Drift Alert: {{model_name}}",
          "textFormat": "{{summary}}"
        }
      ]
    }
  }
}
